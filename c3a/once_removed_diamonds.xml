<tamination>

  <tam title="Once Removed Diamond Circulate"
       from="Once Remove Diamonds">
     <formation>      <dancer gender="girl" y="3" x="0" angle="0"/>      <dancer gender="boy" y="1" x="-1" angle="90"/>      <dancer gender="boy" y="-1" x="-1" angle="90"/>      <dancer gender="girl" y="5" x="0" angle="0"/>    </formation>
    <path couples="2 4">
      <move select="Lead Right" beats="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path couples="1 3">
      <move select="Forward 2"/>
      <move select="Lead Right" beats="2" scaleX="2"/>
    </path>
    <path couples="2 4">
      <move select="Forward 2"/>
      <move select="Lead Right" beats="2" scaleX="2"/>
    </path>
    <path couples="1 3">
      <move select="Lead Right" beats="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
  </tam>

</tamination>
