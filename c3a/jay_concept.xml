<tamination title="Jay Concept">

  <tam title="Jay Square Thru Three" formation="Quarter Tag" group="Jay">
    <path>
      <move select="Extend Left" beats="2" scaleX="1" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="1.5"/>
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleY=".5"/>
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Lead Left" beats="2" scaleX="1.5"/>
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5"/>
      <move select="Extend Right" beats="2" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleX="2"/>
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="2"/>
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2.5"/>
    </path>
  </tam>

  <tam title="Jay Swap Around" formation="Quarter Tag" group="Jay">
    <path>
      <move select="Run Right" beats="4"/>
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 3" beats="4"/>
    </path>
  </tam>

  <tam title="Jay Touch a Quarter" formation="Quarter Tag" group="Jay">
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5"/>
    </path>
  </tam>

</tamination>
